{
  "manifest_version": 2,
  "name": "IRC over Socket.IO",
  "description": "IRC over Socket.IO",
  "version": "0.9.6",
  "minimum_chrome_version": "24",
  "default_locale": "en",
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  },
  "icons": {
    "16": "icon.png",
    "128": "icon.png"
  },
  "permissions": [
    "http://*/*",
    {
      "socket": [
        "tcp-connect",
        "udp-bind",
        "udp-send-to"
      ]
    },
    "storage"
  ]
}
